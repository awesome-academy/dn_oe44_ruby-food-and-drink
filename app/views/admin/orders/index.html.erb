<% provide :title, t("admin.order_manage") %>
<% provide :base_title, t("base.title") %>
<table id="orders" class="table table-hover">
  <thead>
    <tr>
      <th><%= t "order.user_name" %></th>
      <th><%= t "order.status" %></th>
      <th><%= t "order.total" %></th>
      <th><%= t "order.date" %></th>
      <th><%= t "order.action" %></th>
    </tr>
  </thead>
  <tbody id="order_row">
    <%= render(partial: "order", collection: @orders) || t("cart.order_details_empty") %>
  </tbody>
</table>
<div class="row text-center">
  <%= will_paginate @orders %>
</div>
